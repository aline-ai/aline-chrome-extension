{
  "manifest_version": 3,
  "name": "Aline",
  "version": "0.0.0.2",
  "description": "Aline your reading with your writing",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "unlimitedStorage"],
  "host_permissions": ["https://aline-ai--api.modal.run/"]
}
